#img {
  height: 100%;
  width: 100%;
  background-image: url(images/home4.jpg);
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
}

.mainimg {
  font-size: 29px;
  position: absolute;
  display: block;
}

.mainimg > span {
  display: block;
  color: white;
  text-align: left;
  width: 200px;
  height: 33px;
  animation: textanimate 1s linear forwards;
}

.mainimg > span:before {
  display: block;
  width: 200px;
  height: 33px;
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  animation: animate 1s linear forwards;
}

.mainimg > span:after {
  display: block;
  content: "";
  width: 100px;
  height: 33px;
  position :absolute;
  top: 0;
  left: 170px;

  animation: animate 1s linear forwards;
  animation-delay: 1s;
}

@keyframes animate {
    0% {
        width: 0;
        height: 33px;
        border-bottom: 2px solid white;
    }
    100% {
        width: 100%;
        height: 33px;
        border-bottom: 2px solid white;
    }
}

@keyframes textanimate {
    0% {
        transform: translateX(-170px);
        opacity: 0;
    }
    100% {
        transform: translateX(0);
        opacity: 1;
    }

}
